I"ò(function() {
  $(window).load(function() {
    var inventorySolutionBox, locationSolutionBox, solutionBoxes, timeSolutionBox, toggleSelect;
    inventorySolutionBox = document.getElementById('inventory-solution');
    timeSolutionBox = document.getElementById('time-solution');
    locationSolutionBox = document.getElementById('location-solution');
    solutionBoxes = [inventorySolutionBox, timeSolutionBox, locationSolutionBox];
    toggleSelect(inventorySolutionBox);
    toggleSelect = function(currentSolutionBox) {
      var boxUnselect, i, j, len;
      for (i = j = 0, len = solutionBoxes.length; j < len; i = ++j) {
        boxUnselect = solutionBoxes[i];
        if (boxUnselect !== currentSolutionBox) {
          boxUnselect.classList.remove('selected-solution-box');
        } else {
          boxUnselect.classList.add('selected-solution-box');
        }
      }
    };
    inventorySolutionBox.addEventListener('click', function() {
      return toggleSelect(inventorySolutionBox);
    });
    timeSolutionBox.addEventListener('click', function() {
      return toggleSelect(timeSolutionBox);
    });
    return locationSolutionBox.addEventListener('click', function() {
      return toggleSelect(locationSolutionBox);
    });
  });

}).call(this);
