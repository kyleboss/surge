<p id="notice"><%= notice %></p>

<h1>Listing Inventory Snapshot Contents</h1>

<table>
  <thead>
    <tr>
      <th>Inventory snapshot</th>
      <th>Trackable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventory_snapshot_contents.each do |inventory_snapshot_content| %>
      <tr>
        <td><%= inventory_snapshot_content.inventory_snapshot %></td>
        <td><%= inventory_snapshot_content.trackable %></td>
        <td><%= link_to 'Show', inventory_snapshot_content %></td>
        <td><%= link_to 'Edit', edit_inventory_snapshot_content_path(inventory_snapshot_content) %></td>
        <td><%= link_to 'Destroy', inventory_snapshot_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventory snapshot content', new_inventory_snapshot_content_path %>
